<openerp>
	<data>
		<record id="wizard_frequency_mapping_channel_wizard" model="ir.ui.view">
			<field name="name">posix_net.wizard_frequency_mapping_channel_wizard</field>
			<field name="model">posix_net.frequency_mapping_channel_wizard</field>
			<field name="arch" type="xml">
				<form string="Frequency Mapping Channel Wizard" version="7.0">
					<header>
						<button special="cancel" string="Cancel" icon="gtk-cancel" />
						<button name="create_mappings" string="Create" type="object" icon="gtk-go-forward" />
					</header>
					<group>
						<group>
							<field name="mapping_id" />
							<field name="mapping" />
						</group>
						<group>
							<field name="bandwidth" />
							<field name="shifting" />
						</group>
					</group>
					<label for="channel_ids" string="Channels" />
					<field name="channel_ids" />
				</form>
			</field>
		</record>


		<record id="action_frequency_mapping_channel_wizard" model="ir.actions.act_window">
			<field name="name">Channel Wizard</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">posix_net.frequency_mapping_channel_wizard</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="view_id" ref="wizard_frequency_mapping_channel_wizard" />
			<field name="multi">True</field>
			<field name="target">new</field>
		</record>

		<record id="irconfig_frequency_mapping_channel_wizard" model="ir.values">
			<field name="model_id" ref="model_posix_net_frequency_mapping" />
			<field name="model">posix_net.frequency_mapping</field>
			<field name="name">Channel Wizard</field>
			<field name="key2">client_action_multi</field>
			<field name="value" eval="'ir.actions.act_window,' + str(ref('action_frequency_mapping_channel_wizard'))" />
			<field name="key">action</field>
		</record>

	</data>
</openerp>