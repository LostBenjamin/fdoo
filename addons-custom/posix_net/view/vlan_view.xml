<openerp>
	<data>
		<record id="form_posix_net_vlan" model="ir.ui.view">
			<field name="name">posix_net.form_posix_net_vlan</field>
			<field name="model">posix_net.vlan</field>
			<field name="arch" type="xml">
				<form string="VLAN" version="7.0">
					<group>
						<field name="vlan"/>
						<field name="device" />
						<field name="name" />
						<field name="unit_id" invisible="1"/>
						<field name="vlan_port_ids" invisible="1"/>		
					</group>
				</form>
			</field>
		</record>

		<record id="tree_posix_net_vlan" model="ir.ui.view">
			<field name="name">posix_net.tree_posix_net_vlan</field>
			<field name="model">posix_net.vlan</field>
			<field name="arch" type="xml">
				<tree string="VLAN">
					<field name="vlan"/>
					<field name="device" />
					<field name="name" />
				</tree>
			</field>
		</record>

	</data>
</openerp>