<openerp>
	<data>
		
		<record id="form_posix_net_firmware" model="ir.ui.view">
			<field name="name">posix_net.form_posix_net_firmware</field>
			<field name="model">posix_net.firmware</field>
			<field name="arch" type="xml">
				<form string="Firmware" version="7.0">
					<sheet>
						<separator string="Settings" col="4" colspan="4" />
						<group>
							<field name="name" />
							<field name="version" />
							<field name="data" style="width: 100%%"/>
						</group>
						<separator string="Description" />
						<field name="description" />
					</sheet>
				</form>
			</field>
		</record>

		<record id="tree_posix_net_firmware" model="ir.ui.view">
			<field name="name">posix_net.tree_posix_net_firmware</field>
			<field name="model">posix_net.firmware</field>
			<field name="arch" type="xml">
				<tree string="Firmware">
					<field name="name" />
					<field name="version" />
					<field name="data" />
				</tree>
			</field>
		</record>

		<record id="action_posix_net_firmware" model="ir.actions.act_window">
			<field name="name">Firmware</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">posix_net.firmware</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem id="menu_posix_net_firmware" name="Firmware"
			parent="menu_network_config_network_network" sequence="16"
			action="action_posix_net_firmware" />
	</data>
</openerp>