<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<template id="snippets" inherit_id="website.snippets">
    <xpath expr="//div[@id='snippet_structure']" position="inside">
        
        
        <!-- 4 Bild Boxen -->
        
        <div>        
            <div class="oe_snippet_thumbnail">
                <img class="oe_snippet_thumbnail_img" src="/at_website/static/src/img/blocks/block_4columns.png"/>
                <span class="oe_snippet_thumbnail_title">Vier Spalten</span>
            </div>
            <section class="oe_snippet_body">
                <div class="container antomed-4boxen">
                    <div class="row">

                        <!-- a -->
                        <div class="col-md-3 col-sm-6 col-xs-6">
                            <div class="box-wrapper">
                                <div class="box">
                                    <div class="latest-box">

                                        <!-- .media -->
                                        <div class="media">
                                            <!-- image -->
                                            <img src="/antomed/static/src/img/box-fuer-dienstgeber.jpg" class="img-responsive" title="" alt="" />
                                            <!-- image -->
                                        </div>
                                        <!-- .media -->

                                        <!-- content -->
                                        <div class="content-wrapper">
                                            <div class="content">

                                                <h1>FÜR DIENSTGEBER</h1>
                                                <h2>ANTOMED FINDET FÜR SIE DAS PASSENDE PERSONAL FÜR ALLE FACHGEBIETE...</h2>
                                                <h1>MEHR...</h1>

                                            </div>
                                        </div>
                                        <!-- .content -->

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- .a -->

                        <!-- b -->
                        <div class="col-md-3 col-sm-6 col-xs-6">
                            <div class="box-wrapper">
                                <div class="box">
                                    <div class="latest-box">

                                        <!-- .media -->
                                        <div class="media">
                                            <!-- image -->
                                            <img src="/antomed/static/src/img/box-fuer-aerzte.jpg" class="img-responsive" title="" alt="" />
                                            <!-- image -->
                                        </div>
                                        <!-- .media -->

                                        <!-- content -->
                                        <div class="content-wrapper">
                                            <div class="content">

                                                <h1>FÜR ÄRZTE</h1>
                                                <h2>KOSTENLOS REGISTRIEREN UND VON ANTOMED PROFITIEREN...</h2>
                                                <h1>MEHR...</h1>

                                            </div>
                                        </div>
                                        <!-- .content -->

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- .b -->


                        <!-- c -->
                        <div class="col-md-3 col-sm-6 col-xs-6">
                            <div class="box-wrapper">
                                <div class="box">
                                    <div class="latest-box">

                                        <!-- .media -->
                                        <div class="media">
                                            <!-- image -->
                                            <img src="/antomed/static/src/img/box-ihre-vorteile.jpg" class="img-responsive" title="" alt="" />
                                            <!-- image -->
                                        </div>
                                        <!-- .media -->

                                        <!-- content -->
                                        <div class="content-wrapper">
                                            <div class="content">

                                                <h1>IHRE VORTEILE</h1>
                                                <h2>PROFITEREN AUCH SIE VON ERFAHRUNG, KOMPETENZ UND EINEM GROSSEN NETZWERK...</h2>
                                                <h1>MEHR...</h1>

                                            </div>
                                        </div>
                                        <!-- .content -->

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- .c -->

                        <!-- d -->
                        <div class="col-md-3 col-sm-6 col-xs-6">
                            <div class="box-wrapper">
                                <div class="box">
                                    <div class="latest-box">

                                        <!-- .media -->
                                        <div class="media">
                                            <!-- image -->
                                            <img src="/antomed/static/src/img/box-in-sicheren-haenden.jpg" class="img-responsive" title="" alt="" />
                                            <!-- image -->
                                        </div>
                                        <!-- .media -->

                                        <!-- content -->
                                        <div class="content-wrapper">
                                            <div class="content">

                                                <h1>IN SICHEREN HÄNDEN:</h1>
                                                <h2>IHRE DATEN SIND 100% SICHER UND WERDEN ABSOLUT VERTRAULICH BEHANDELT...</h2>
                                                <h1>MEHR...</h1>

                                            </div>
                                        </div>
                                        <!-- .content -->

                                    </div>
                                </div>
                            </div>                            
                        </div>
                    </div>
                </div>                                           
            </section>            
        </div>
        
        
        <!-- Slider Startseite -->
        
        <div>
            <div class="oe_snippet_thumbnail">
                <img class="oe_snippet_thumbnail_img" src="/at_website/static/src/img/blocks/block_banner.png"/>
                <span class="oe_snippet_thumbnail_title">Home Slider</span>
            </div>
            <div id="myCarousel" class="oe_snippet_body carousel slide mb32 home-slider">
                <!-- Indicators -->
                <ol class="carousel-indicators hidden">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="/antomed/static/src/img/home-header.jpg" />
                    </div>
                </div>
                <div class="carousel-control left hidden" data-target="#myCarousel" data-slide="prev" style="width: 10%"><i class="fa fa-chevron-left"></i></div>
                <div class="carousel-control right hidden" data-target="#myCarousel" data-slide="next" style="width: 10%"><i class="fa fa-chevron-right"></i></div>
            </div>
        </div>
        
        
        <!-- Headerbilder Unterseiten -->
        
        <div>
            <div class="oe_snippet_thumbnail">
                <img class="oe_snippet_thumbnail_img" src="/at_website/static/src/img/blocks/block_banner.png"/>
                <span class="oe_snippet_thumbnail_title">Seiten-Header</span>
            </div>
            <div id="myCarousel" class="oe_snippet_body carousel slide mb32 page-header">
                <!-- Indicators -->
                <ol class="carousel-indicators hidden">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="item active">
                        
                        <div class="container">
                            <div class="row">
                                <img src="/antomed/static/src/img/page-header.jpg" />
                            </div>
                        </div>
                         
                    </div>
                </div>
                <div class="carousel-control left hidden" data-target="#myCarousel" data-slide="prev" style="width: 10%"><i class="fa fa-chevron-left"></i></div>
                <div class="carousel-control right hidden" data-target="#myCarousel" data-slide="next" style="width: 10%"><i class="fa fa-chevron-right"></i></div>
            </div>
        </div>
    
    </xpath>
</template>


</data>
</openerp>
