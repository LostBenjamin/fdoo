    	<record id="product_normal_form_view" model="ir.ui.view">
            <field name="name">product.normal.form</field>
            <field name="model">product.product</field>
            <field name="type">form</field>
            <field eval="7" name="priority"/>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
            	<data>
            		<group name="volume_and_weight" position="replace">            			
            		</group>
            		            		
            		<field name="variants" position="replace">
            			<field name="variants" />
            		</field>
            		
            		<page string="Information" position="after">
            			<page string="Attributes">
            				<group name="volume_and_weight" colspan="2" col="2" groups="base.group_extended">
                                <separator string="Weights" colspan="2"/>
                                <field digits="(14, 3)" name="volume" attrs="{'readonly':[('type','=','service')]}"/>
                                <field digits="(14, 3)" name="weight" attrs="{'readonly':[('type','=','service')]}"/>
                                <field digits="(14, 3)" name="weight_net" attrs="{'readonly':[('type','=','service')]}"/>
                            </group>
                            <group name="dimension" colspan="2" col="2">
                            	<separator string="Dimension" colspan="2"/>
                            	<field name="pr_unit"/>
                            	<field name="pr_format_id"/>
                            	<field name="pr_leteral_pressure_id"/>     
                            	<field name="pr_gram_id"/>                       	
                            </group>        				
                            <group name="material" colspan="2" col="2">
                            	<separator string="Material" colspan="2"/>
                            	<field name="pr_material_id"/>
                            	<field name="pr_material_surface_id"/>
                            </group>
                            <group name="orientation_color" colspan="2" col="2">
                            	<separator string="Orientation &amp; Color" colspan="2"/>
                            	<field name="pr_orientation_id"/>           
                            	<field name="pr_color_id"/>                 	
                            </group>  
                            <group name="additional_attribs" colspan="4" col="2">                          
	                            <separator string="Additional Attributes" colspan="2"/>
	                            <field name="pr_attribute_ids"/>
	                        </group>
            			</page>            			
            		</page>         		
            	</data>
            </field>
