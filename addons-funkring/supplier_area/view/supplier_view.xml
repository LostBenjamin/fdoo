        <record id="tree_purchase_line" model="ir.ui.view">
            <field name="name">supplier.tree_purchase_line</field>
            <field name="model">purchase.order.line</field>
            <field name="type">tree</field>
            <field name="priority">100</field>            
            <field name="arch" type="xml">
                <tree string="Purchase Order Lines"
                	  colors="red:(date_planned&lt;current_date);red:(severity=='2');red:(priority=='3');orange:(severity=='1');orange:(priority=='2');black:(priority=='1');gray:(priority=='0')">                	
                	<button name="stage_next" string="Next Stage" states="draft,confirmed"  type="object"  icon="gtk-go-forward" attrs="{'invisible':[('state','==','done')]}"/>
                	<button name="show_info" string="Info" type="object" icon="terp-stage"/>
               	 	<button name="dummy" string="Ship" type="object" attrs="{'invisible':['|',('supplier_ships','!=',True),('state','!=','confirmed')]}"
                              			icon="terp-accessories-archiver"/>	                           
                	<field name="supplier_ships" invisible="1"/>                	                	
                	<field name="date_planned"  width="135"/>
                	<field name="category_id"/>                	
                	<field name="shop_id"/>    
                	<field name="state"/>
                	<field name="severity" invisible="1"/>                	
                	<field name="level_id"/>              
                	<field name="write_date"/>  	
                	<field name="priority"/>                	
                    <field name="name"/> 
                    <field name="client_order_ref" />
                    <field name="dest_address_id"/>  
                    <field name="product_qty"/>
                    <field name="product_uom"/>                    
                    <field name="supplier_invoiced"/>
                    <field name="origin"/>
                </tree>
            </field>
        </record>
        
