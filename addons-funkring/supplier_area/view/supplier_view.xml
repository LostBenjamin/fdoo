<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>    	
    	 <record id="form_purchase_line" model="ir.ui.view">
            <field name="name">supplier.form_purchase_line</field>
            <field name="model">purchase.order.line</field>
            <field name="type">form</field>            
            <field name="priority">100</field>
            <field name="arch" type="xml">
                <form string="Purchase Order Line">              		
                    <notebook colspan="4">
                        <page string="Order Line">                  	
                        	<field name="origin" readonly="1"/>                        	         	                	
                        	<field name="date_planned" readonly="1"/>                        	      
                        	<field name="name" readonly="1"/>
                        	<field name="client_order_ref" />                    	
                            <field name="product_qty" readonly="1"/>
                            <field name="product_uom" readonly="1"/>                            
                            <field name="price_unit" readonly="1"/>
                            <field name="supplier_unit_price" readonly="1"/>                            
                            <newline/>
                            <field name="priority" readonly="1" colspan="1"/>                                                        
                            <group colspan="2" col="6">         
                            	<field name="level_id" widget="selection" readonly="1" colspan="1"/>
	                            <button name="stage_next" string="Next Stage"  type="object"  states="draft,confirmed" icon="gtk-go-forward"/>
	                            <field name="supplier_invoiced" readonly="1"/>
                            </group>                     
                            <field name="log_ids" readonly="1" nolabel="1" colspan="4">
                            	<form string="Log Entry">
                            		<group colspan="4" col="6">
	                            		<field name="create_date"/>
	                            		<field name="user_id"/>
	                            		<field name="severity"/>
	                            	</group>
                            		<separator string="Message" colspan="4"/>
                            		<field name="name" widget="text" nolabel="1"/>
                            	</form>
                            	<tree string="Log Entries">
                            		<field name="create_date"/>
	                            	<field name="user_id"/>
	                            	<field name="severity"/>
	                            	<field name="name"/>	                            
                            	</tree>                            	
                            </field>
                            <group col="10" colspan="4">
                                <field name="state" readonly="True"/>                                
                                <button icon="gtk-print" name="%(at_stock_report.report_picking_list_purchase_line_pdf)d" string="Picking" type="action"/>
                                <button icon="gtk-print" name="%(at_stock_report.report_picking_list_small_pdf)d" string="Picking Small" type="action"/>            
                                <button icon="gtk-add" name="open_attachments" string="Attachments" type="object"/>                                
                            </group>
                        </page>                        
                         <page string="Notes">                			 
                            <field name="notes" readonly="1" colspan="4" nolabel="1"/>
                        </page>                       
                    </notebook>
                </form>
            </field>
        </record>
        
        <record id="form_purchase_line2" model="ir.ui.view">
            <field name="name">supplier.form_purchase_line2</field>
            <field name="model">purchase.order.line</field>
            <field name="type">form</field>            
            <field name="priority">100</field>
            <field name="arch" type="xml">
                <form string="Purchase Order Line">
              		<group name="purchase_order_group"  colspan="4" col="4">            
              			<separator colspan="4" string="Purchase Order"/>		                		
                		<field name="order_id" readonly="1"/>    			
                		<field name="partner_id" readonly="1"/>
                		<field name="shop_id" readonly="1"/>                		
                	</group>
                    <notebook colspan="4">
                        <page string="Order Line">                  	
                        	<field name="origin" attrs="{'readonly':[('state','!=','draft')]}"/>                        	         	                	
                        	<field name="date_planned" attrs="{'readonly':[('state','!=','draft')]}"/>                        	      
                        	<field name="name" colspan="4" attrs="{'readonly':[('state','!=','draft')]}"/>                    	
                            <field name="product_qty" attrs="{'readonly':[('state','!=','draft')]}"/>
                            <field name="product_uom" attrs="{'readonly':[('state','!=','draft')]}"/>                            
                            <field name="price_unit" attrs="{'readonly':[('state','!=','draft')]}"/>
                            <field name="supplier_unit_price" attrs="{'readonly':[('state','!=','draft')]}"/>                            
                            <newline/>
                            <field name="priority" attrs="{'readonly':[('state','!=','draft')]}" colspan="1"/>                                                        
                            <group colspan="2" col="6">         
                            	<field name="level_id" widget="selection" readonly="1" colspan="1"/>
	                            <button name="stage_next" string="Next Stage"  type="object"  states="draft,confirmed" icon="gtk-go-forward"/>
	                            <field name="supplier_invoiced" readonly="1"/>
                            </group>                     
                            <field name="log_ids" readonly="1" nolabel="1" colspan="4">
                            	<form string="Log Entry">
                            		<group colspan="4" col="6">
	                            		<field name="create_date"/>
	                            		<field name="user_id"/>
	                            		<field name="severity"/>
	                            	</group>
                            		<separator string="Message" colspan="4"/>
                            		<field name="name" widget="text" nolabel="1"/>
                            	</form>
                            	<tree string="Log Entries">
                            		<field name="create_date"/>
	                            	<field name="user_id"/>
	                            	<field name="severity"/>
	                            	<field name="name"/>	                            
                            	</tree>                            	
                            </field>
                            <group col="6" colspan="4">
                                <field name="state" readonly="True"/>
                                <label string="" colspan="2"/>
                                <button name="open_attachments" string="Attachments" type="object"/>
                                <button name="confirm" string="Validate" type="object" states="draft" icon="gtk-go-forward"/>                                
                            </group>
                        </page>                        
                         <page string="Notes">                			 
                            <field name="notes" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}"/>
                         </page>                       
                    </notebook>
                </form>
            </field>
        </record>
        
