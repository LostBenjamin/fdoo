       	<record id="search_purchase_line" model="ir.ui.view">
            <field name="name">supplier.search_purchase_line</field>
            <field name="model">purchase.order.line</field>
            <field name="type">search</field>                    
            <field name="arch" type="xml">
                <search string="Search Purchase Order">
                        <group colspan="4" col="8">	    
                        	<filter icon="terp-document-new" name="filter_draft" string="Draft" domain="[('state','=','draft')]" separator="1" help="Draft Purchase line"/>
	                        <filter icon="terp-camera_test" name="filter_confirmed" string="Confirmed" domain="[('state','=','confirmed')]" separator="1" help="Confirmed Purchase line"/>       
	                        <filter name="today"
									string="Today Changed"
									icon="terp-go-month"
									domain="[('write_date','&gt;=',(datetime.date.today()).strftime('%%Y-%%m-%%d')), ('write_date','&lt;',(datetime.date.today()+datetime.timedelta(days=1)).strftime('%%Y-%%m-%%d'))]"/> 
							<filter name="today"
									string="Today Planned"
									icon="terp-go-month"
									domain="[('date_planned','&gt;=',(datetime.date.today()).strftime('%%Y-%%m-%%d')), ('date_planned','&lt;',(datetime.date.today()+datetime.timedelta(days=1)).strftime('%%Y-%%m-%%d'))]"/>                                   
                        	<field name="origin" string="Origin"/>                        	
                        	<field name="category_id" widget="selection" operator="child_of"/>
							<field name="client_order_ref" />                        	
                        	<field name="dest_address_id" />      
	                        <field name="level_id"/>     
	                        <field name="date_planned"/>
	                                          
                        </group>
                        
                        <newline/>
                        <group expand="0" string="Group By...">
	                        <filter string="Category" icon="terp-stock" domain="[]" context="{'group_by' : 'category_id'}" />
	                        <filter string="Date" icon="terp-calendar" domain="[]" context="{'group_by' : 'date_planned'}" />
	                        <filter string="Priority" icon="terp-project" domain="[]" context="{'group_by' : 'priority'}" /> 
	                        <separator orientation="vertical"/>
	                        <filter string="State" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by' : 'state'}" />
                        	<separator orientation="vertical"/>
                        	<filter string="Delivery Address" icon="terp-personal" domain="[]" context="{'group_by' : 'dest_address_id'}" />
                        	<filter string="Customer Reference" icon="terp-personal" domain="[]" context="{'group_by' : 'client_order_ref'}" />
                        	<filter string="Shop" icon="gtk-home" domain="[]" context="{'group_by' : 'shop_id'}" />
                        	<field name="shop_id" />
                        </group>
               </search>
         	</field>
         </record> 
         
