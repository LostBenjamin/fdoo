            <field name="partner_to">${",".join(object.student_id.partner_ids}</field>
            <field name="model_id" ref="academy.model_academy_registration"/>
            <field name="body_html"><![CDATA[
<h1>Thank your for Registration</h1>
            ]]></field>
     </record>
  </data>
</openerp>
