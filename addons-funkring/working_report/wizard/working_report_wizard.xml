<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <record id="wizard_working_report" model="ir.ui.view">
      <field name="name">working_report.wizard_working_report</field>
      <field name="model">working.report.wizard</field>
      <field name="arch" type="xml">
        <form string="Working Report Wizard" version="7.0">
          <group>
            <group>
              <field name="description"/>
            </group>
          </group>
          <footer>
            <button icon="gtk-go-forward" name="action_working_report" string="Create"  type="object" class="oe_highlight" />
            or
            <button special="cancel" string="Close" class="oe_link" />
          </footer>
        </form>
      </field>
    </record>
    
    <act_window id="action_wizard_working_report"
      multi="True"
      key2="client_action_multi"
      name="Create Working Report"
      res_model="working.report.wizard"
      src_model="account.analytic.line"
      view_mode="form"
      target="new"
      view_type="form"
      view_id="wizard_working_report"/>

  </data>
</openerp>
