<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <record id="wizard_working_report" model="ir.ui.view">
      <field name="name">working_report.wizard_working_report</field>
      <field name="model">working.report.wizard</field>
      <field name="arch" type="xml">
        <form string="Transport Wizard" version="7.0">
          <group>
            <group>
              <field name="description"/>
            </group>
          </group>
          <footer>
            <button icon="gtk-go-forward" name="action_working_report" string="string"  type="object" class="oe_highlight" />
            or
            <button special="cancel" string="Close" class="oe_link" />
          </footer>
        </form>
      </field>
    </record>
  </data>
</openerp>
